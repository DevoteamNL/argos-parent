{
  "create": "service-accounts-key-info-tmp",
  "viewOn": "serviceAccounts",
  "pipeline": [
    {
      "$lookup": {
        "from": "hierarchy",
        "localField": "accountId",
        "foreignField": "referenceId",
        "as": "joinedhierarchy"
      }
    },
    {
      "$project": {
        "accountId": 1.0,
        "name": 1.0,
        "inactiveKeys": "$inactiveKeyPairs.keyId",
        "activeKey": {
          "$cond": [
            {
              "$ne": [
                {
                  "$ifNull": [
                    "$activeKeyPair.keyId",
                    ""
                  ]
                },
                ""
              ]
            },
            [
              {
                "keyId": "$activeKeyPair.keyId",
                "status": "ACTIVE"
              }
            ],
            null
          ]
        },
        "accountType": {
          "$arrayElemAt": [
            "$joinedhierarchy.type",
            0.0
          ]
        },
        "pathToRoot": {
          "$arrayElemAt": [
            "$joinedhierarchy.pathToRoot",
            0.0
          ]
        },
        "_class": "com.argosnotary.argos.domain.account.AccountKeyInfo"
      }
    }
  ]
}